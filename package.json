{
  "name": "codewars-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register --files src/main.ts",
    "start:dev": "nest start -w",
    "start:debug": "concurrently --handle-input \"wait-on dist/main.js && nodemon --config nodemon-debug.json\" \"tsc -w -p tsconfig.build.json\" ",
    "start:prod": "node dist/main.js",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./src ./test",
    "lint:compile": "tsc",
    "semantic-release": "semantic-release",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "NODE_ENV=test jest --config ./test/jest-e2e.json --runInBand --forceExit",
    "test:e2e:local": "POSTGRES_DB=test_db yarn test:e2e --verbose",
    "seed": "ts-node seeds/seeder.ts",
    "typeorm": "./node_modules/typeorm/cli.js -f dist/config/typeorm.config -c default",
    "db:migrations:create": "yarn typeorm migration:create",
    "db:migrations:generate": "yarn typeorm migration:generate",
    "db:migrations:run": "yarn typeorm migration:run -t=each",
    "db:migrations:rollback": "yarn typeorm migration:revert -t=each",
    "check:all": "yarn lint && yarn lint:compile && yarn test && yarn test:e2e:local"
  },
  "dependencies": {
    "@nestjs/common": "^7.6.13",
    "@nestjs/config": "^0.6.1",
    "@nestjs/core": "^7.6.13",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.6.13",
    "@nestjs/swagger": "^4.8.0",
    "@nestjs/typeorm": "^7.1.5",
    "@sentry/node": "^6.3.0",
    "@sentry/tracing": "^6.3.0",
    "@types/faker": "^5.5.1",
    "@types/react-router-dom": "^5.1.7",
    "argon2": "^0.27.1",
    "class-sanitizer": "^1.0.1",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "csvtojson": "^2.0.10",
    "express": "^4.17.1",
    "express-formidable": "^1.2.0",
    "express-session": "^1.17.1",
    "nestjs-typeorm-paginate": "^2.3.2",
    "passport": "^0.4.1",
    "pg": "^8.5.1",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.3",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.2.32",
    "typeorm-naming-strategies": "^2.0.0",
    "typeorm-store": "^1.2.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.6",
    "@nestjs/testing": "^7.6.13",
    "@semantic-release/git": "^9.0.0",
    "@types/express": "^4.17.1",
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.33",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "concurrently": "^6.0.0",
    "eslint": "^7.20.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsdoc": "^32.0.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "faker": "^5.5.1",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "semantic-release": "^17.4.2",
    "supertest": "^6.1.3",
    "ts-jest": "26.5.3",
    "ts-node": "9.1.1",
    "tsconfig-paths": "3.9.0",
    "typescript": "4.2.3",
    "wait-on": "^5.2.1"
  },
  "resolutions": {
    "typeorm": "^0.2.32"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
